
#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

void main()
{
int a,b,c;
float s,Area;

cout<< "introducir lado1: ";
cin>>  a;
cout<< "Introducir lado2: ";
cin>>  b;
cout<< "Introducir lado3: ";
cin>>  c;

s= (a+b+c)/2;
Area= sqrt(s*(s-a)*(s-b)*(s-c));
cout<< "El area es: "<<Area;

getch();
}

